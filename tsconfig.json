{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": false,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"module": "CommonJS",
		"target": "es5",
		"allowJs": true,
		"outDir": "dist",
		"declaration": true,
		"baseUrl": ".",
		"moduleResolution": "Node",
		"paths": {
			"json-to-ts": ["node_modules/json-to-ts/build/src"],
			"typescript-json-schema": ["node_modules/typescript-json-schema/dist"]
		}
	},
	"include": ["./src", "node-http-proxy-json.d.ts"]
}
